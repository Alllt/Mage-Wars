			<location x="325" y="50" />
			<text color="#000000" size="18" />
			<wordwrap height="80" width="70" />
		</block>
		<block type="text" id="Level">
			<location x="325" y="130" />
			<text color="#000000" size="15" />
		</block>
		<block type="text" id="Stats">
			<location x="75" y="250" />
			<text color="#000000" size="20" />
		</block>
		<block type="text" id="AttackBar">
			<location x="1" y="335" />
			<text color="#000000" size="17" />
			<wordwrap height="40" width="400" />
		</block>
		<block type="text" id="Traits">
			<location x="1" y="380" />
			<text color="#000000" size="18" />
			<wordwrap height="40" width="400" />
		</block>
		<block type="text" id="Text">
			<location x="1" y="425" />
			<text color="#000000" size="12" />
			<wordwrap height="72" width="400" />
		</block>
		<block type="text" id="CardID">
			<location x="160" y="525" />
			<text color="#000000" size="15" />
		</block>
	</blocks>

